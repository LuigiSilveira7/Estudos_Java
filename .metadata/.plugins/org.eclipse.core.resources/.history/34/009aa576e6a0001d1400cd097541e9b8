package estudos;

public class calculadorDePorcentagem {
	public static void main (String[] args) {
		float salario = 2403.98F;
		float porcentagem = 30;
		float resultado = salario * porcentagem / 100;
		System.out.println("O resultado de 30% do seu salario eh de " + resultado);
		System.out.println(" caralhados " + porcentagem);
				
	}

}